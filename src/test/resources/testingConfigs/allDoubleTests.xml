<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="allDoubleSuite" verbose="1">
    <test name="allDoubleTests">
        <groups>
            <run>
                <include name="multAndDivTests"/>
                <include name="sumAndSubTests"/>
            </run>
        </groups>
        <classes>
            <class name="ru.training.at.hw1.MultiplicationDoubleTest"/>
            <class name="ru.training.at.hw1.DivisionDoubleTest"/>
            <class name="ru.training.at.hw1.SubtractionDoubleTest"/>
            <class name="ru.training.at.hw1.AdditionDoubleTest"/>
        </classes>
    </test>
</suite>
